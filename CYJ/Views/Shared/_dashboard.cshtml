<!DOCTYPE html>

<html>

<head>
 
    <!--Template Header-->
    <meta name="viewport" content="width=device-width" />
    
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="@Url.Content("~/Content/mytemplate/")img/apple-icon.png" />
    <link rel="icon" type="image/png" href="@Url.Content("~/Content/mytemplate/")img/favicon.png" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewBag.Title</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <!-- Bootstrap core CSS     -->
    <link href="@Url.Content("~/Content/mytemplate/")css/bootstrap.min.css" rel="stylesheet" />
    <!--  Material Dashboard CSS    -->
    <link href="@Url.Content("~/Content/mytemplate/")css/material-dashboard.css?v=1.2.0" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="@Url.Content("~/Content/mytemplate/")css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>

    <!--Template Scripts -->
    <!--   Core JS Files   -->
    <script src="@Url.Content("~/Content/mytemplate/")js/jquery-3.2.1.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/bootstrap.min.js" type="text/javascript"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/material.min.js" type="text/javascript"></script>
    <!--  Charts Plugin -->
    <script src="@Url.Content("~/Content/mytemplate/")js/chartist.min.js"></script>
    <!--  Dynamic Elements plugin -->
    <script src="@Url.Content("~/Content/mytemplate/")js/arrive.min.js"></script>
    <!--  PerfectScrollbar Library -->
    <script src="@Url.Content("~/Content/mytemplate/")js/perfect-scrollbar.jquery.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="@Url.Content("~/Content/mytemplate/")js/bootstrap-notify.js"></script>
    <!--  Google Maps Plugin    -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>
    <!-- Material Dashboard javascript methods -->
    <script src="@Url.Content("~/Content/mytemplate/")js/material-dashboard.js?v=1.2.0"></script>
    <!-- Material Dashboard DEMO methods, don't include it in your project! -->
    <script src="@Url.Content("~/Content/mytemplate/")js/demo.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            // Javascript method's body can be found in assets/js/demos.js
            demo.initDashboardPageCharts();

        });
    </script>
    <script>
        div.scroll {
            background - color: #00FFFF;
            width: 100px;
            height: 100px;
            overflow: scroll;
        }
    </script>

</head>
<body>

    <img src="~/Content/Images/CITYYEARLOGO.png" alt="LOGO" runat="server" />
    <div class="wrapper">
        <div class="main-panel">
            <div class="scroll">
            <!-- changes sidebars color and background image-->
            <div class="sidebar" data-color="red" data-image="@Url.Content("~/Content/mytemplate/")img/sidebar-1.jpg">
                <div class="logo">
                    <a href="https://www.cityyear.org/jacksonville" class="simple-text">
                        City Year Jax
                    </a>
                </div><!-- ends logo -->
                <div class="sidebar-wrapper">
                    <ul class="nav">
                        <li class="active">
                            @Html.ActionLink("Dashboard", "Index", "Home")
                        </li>
                        <li>

                            @Html.ActionLink("Profile", "Index", "Manage")




                        </li>
                        <li>
                            @Html.ActionLink("About Us", "About", "Home")
                        </li>
                        <li>
                            @Html.ActionLink("Department", "Access", "Home")
                        </li>
                    </ul>
                </div> <!-- ends side bar nav-->
            </div> <!-- ends pics on side bar nav -->
            <!-- End of Sidebar-->
            <!-- Top Nav Bar-->
            <div class="topnav">
                    @Html.Partial("_LoginPartial")
                </div>
            </div>

            <!-- End Top Nav Bar-->

            <div>
                @RenderBody()
            </div>
        </div>
    </div>
</body>
</html>
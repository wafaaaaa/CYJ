@model IEnumerable<CYJ.Models.WORKSTREAM>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_mytemplate.cshtml";
}

<h2>Workstreams</h2>

<p>
    @Html.ActionLink("Create a new workstream", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.workstreamName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.workstreamName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.workstreamID })
        </td>
    </tr>
}

</table>
